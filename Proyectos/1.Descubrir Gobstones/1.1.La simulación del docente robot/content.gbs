program { 
    RepartirCaramelos()
    RecolectarEnvoltorios()
}

procedure RepartirCaramelos() {
    repeat (19) {
        IrAlPróximoEstudiante()
        DarUnCaramelo() 
    }
    IrAlEscritorio()
    ComerLosCaramelos()
}

procedure RecolectarEnvoltorios() {    
    repeat (19) {
        IrAlPróximoEstudiante()
        TomarElEnvoltorio() 
    }
    IrAlEscritorio()
}
